# Environment
NODE_ENV=development
NEXT_PUBLIC_USE_PROD_DATA=false
NEXT_PUBLIC_DATA_VERSION=mvp

# User Configuration
NEXT_PUBLIC_DEFAULT_USER_ID=f5cb0287-d141-4f8b-9632-98be8d7bcbe7

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_METRICS=true
NEXT_PUBLIC_ENABLE_FEEDBACK=true
NEXT_PUBLIC_ENABLE_PORTFOLIO=true

# API Configuration
NEXT_PUBLIC_PRAISON_AI_URL=http://localhost:8000
NEXT_PUBLIC_MAX_TOKENS=100000
